{
    "name": "BAD ROBLOX UPDATE REVERTER",
    "description": "Helps Revert Bad Roblox Website Updates",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "structure.html",
      "default_icon": "BadRoblox.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "background": {
    "service_worker": "background.js"
    },
    "content_scripts": [
        {
        "matches": ["*://*.roblox.com/*"],
        "js": ["main.js"]
        }
    ]
}